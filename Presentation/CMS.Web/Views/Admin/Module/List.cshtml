@using CMS.Data.Service.Helpers;
@using CMS.Data.Service.Details;
@using CMS.Web.FrameWork.Helpers;
@model PagedList<ModuleDetail>
@{
	GridViewResult<ModuleDetail> gridViewResult = new GridViewResult<ModuleDetail>();
	gridViewResult.rows = Model;
	gridViewResult.total = Model.TotalCount;
	gridViewResult.totalpages = Model.TotalPages;
	Response.Write(JsonHelper.JsonSerializer<GridViewResult<ModuleDetail>>(gridViewResult));
}